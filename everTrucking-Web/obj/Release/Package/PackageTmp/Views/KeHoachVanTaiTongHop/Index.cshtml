<script src="https://cdnjs.cloudflare.com/ajax/libs/cleave.js/1.6.0/cleave.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {

        var datePicker = document.querySelectorAll(".ngay-vn");
        for (let i = 0; i < datePicker.length; i++) {
            $('#' + datePicker[i].getAttribute("id")).datepicker({ format: 'dd/mm/yyyy', language: 'vi' });
        };

        var cleaveDate = document.querySelectorAll(".ngay-vn");
        for (let i = 0; i < cleaveDate.length; i++) {
            var cleave = new Cleave(cleaveDate[i], {
                date: true,
                delimiter: '/',
                datePattern: ['d', 'm', 'Y']
            });
        };

        var today = new Date();
        var dd = ("0" + (today.getDate())).slice(-2);
        var mm = ("0" + (today.getMonth() + 1)).slice(-2);
        var yyyy = today.getFullYear();
        var strtoday = dd + '/' + mm + '/' + yyyy;


        $("#txtTuNgay").val(strtoday);
        $("#txtDenNgay").val(strtoday);

        loadData();
    });

    function loadData() {

        var data = {
            startDate: $('#txtTuNgay').val(),
            endDate: $('#txtDenNgay').val(),
            LoaiHinh: $('#txtLoaiHinh').val(),
            LoaiHang: $('#txtLoaiHang').val()
        };
        //console.log(JSON.stringify(data));
        if ($.fn.dataTable.isDataTable('#table3')) {
            $('#table3').dataTable().fnDestroy();
        }
        $.ajax({
            url: "@Url.Action("List","KeHoachVanTaiTongHop")",
            data: JSON.stringify(data),
            type: "POST",
            contentType: "application/json;charset=utf-8",
            dataType: "json",

            success: function (result) {
                var html = '';
                $.each(result, function (key, item) {
                    html += '<tr>';
                    html += '<td>' + item.So + '</td>';
                    html += '<td class="ngaylap">' + item.NgayLap + '</td>';
                    html += '<td >' + item.TenLoaiHinh + '</td>';
                    html += '<td >' + item.TenLoaiHang + '</td>';
                    html += '<td >' + item.TenDanhMucKhachHang + '</td>';
                    html += '<td >' + item.TenDanhMucHangTau + '</td>';
                    html += '<td >' + item.TenDanhMucDiaDiemNangCont + '</td>';
                    html += '<td >' + item.NgayNangCont + '</td>';
                    html += '<td >' + item.TenDanhMucDiaDiemHaCont + '</td>';
                    html += '<td >' + item.NgayHaCont + '</td>';
                    html += '<td >' + item.TenDanhMucDiaDiemDongHang + '</td>';
                    html += '<td >' + item.NgayDongHang + '</td>';
                    html += '<td >' + item.TenDanhMucDiaDiemTraHang + '</td>';
                    html += '<td >' + item.NgayTraHang + '</td>';
                    html += '<td >' + item.TenDanhMucNguoiSuDungCreate + '</td>';
                    html += '<td><a href="#" onclick="return getbyID(' + item.ID + ')"><i class="fa fa-pen fa-1x mr-1"></i></a> | <a href="#" onclick="Delele(' + item.ID + ')"><i class="fa fa-trash  fa-1x ml-1"></i></a></td>';
                    html += '</tr>';

                });
                $('.tbody').html(html);
                $('#table3').DataTable();
            },
            error: function (errorMesage) {
                alert(errorMesage.responseText);
            }

        });

    }
    function Add() {
        var empObj = {
        // ID: $('#ID').val(),
        //IDDanhMucDonVi: $('#IDDanhMucDonVi').val(),
        // IDDanhMucLoaiDoiTuong: $('#IDDanhMucLoaiDoiTuong').val(),
            NgayLap: $('#NgayLap').val(),
            IDDanhMucSale: $('#IDDanhMucSale').val(),
            IDDanhMucKhachHang: $('#IDDanhMucKhachHang').val(),
            LoaiHinh: $('#LoaiHinh').val(),
            LoaiHang: $('#LoaiHang').val(),
            IDDanhMucHangTau: $('#IDDanhMucHangTau').val(),
            IDDanhMucDiaDiemNangCont: $('#IDDanhMucDiaDiemNangCont').val(),
            NgayNangCont: $('#txtNgayNang').val(),
            IDDanhMucDiaDiemHaCont: $('#IDDanhMucDiaDiemHaCont').val(),
            NgayHaCont: $('#txtNgayHa').val(),
            SoLuongCont20: $('#SoLuongCont20').val(),
            SoCont20: $('#SoCont20').val(),
            SoLuongCont40: $('#SoLuongCont40').val(),
            SoCont40: $('#SoCont40').val(),
            SoLuongCont45: $('#SoLuongCont45').val(),
            SoCont45: $('#SoCont45').val(),
            SoLuongContOpenTop: $('#SoLuongContOpenTop').val(),
            SoContOpenTop: $('#SoContOpenTop').val(),
            SoLuongContFlatRack: $('#SoLuongContFlatRack').val(),
            SoContFlatRack: $('#SoContFlatRack').val(),
            IDDanhMucDiaDiemDongHang: $('#IDDanhMucDiaDiemDongHang').val(),
            NgayDongHang: $('#txtNgayDong').val(),
            IDDanhMucDiaDiemTraHang: $('#IDDanhMucDiaDiemTraHang').val(),
            NgayTraHang: $('#txtNgayTra').val(),
            KhoiLuong: $('#KhoiLuong').val(),
            NguoiGiaoNhan: $('#NguoiGiaoNhan').val(),
            SoDienThoaiGiaoNhan: $('#SoDienThoaiGiaoNhan').val(),
            GhiChu: $('#GhiChu').val()
        };
        var url = "@Url.Action("Add","KeHoachVanTaiTongHop")"
        $.ajax({
        url: url,
            data: JSON.stringify(
                empObj
            ),
            type: "POST",
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (result) {
                loadData();
                alert(result.Message);
                $('#myModal3').modal('hide');
             },
            error: function (errormessage) {
                alert(errormessage.responseText);
            }
        });

    }


    function getbyID(EmpID) {

        $.ajax({
            url: "/KeHoachVanTaiTongHop/getbyID/" + EmpID,
            typr: "GET",
            contentType: "application/json;charset=UTF-8",

            dataType: "json",
            success: function (result) {

                $('#ID').val(result.ID);
